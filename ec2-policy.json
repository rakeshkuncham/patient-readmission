  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3ModelRead",
      "Effect": "Allow",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::patient-risk-data-rakesh-2025/models/*"
    },
    {
